#include<stdio.h>
#include<math.h>
#include<string.h>
#include<stdlib.h>
int main()
    {
   char alph;
scanf("%c",&alph);
switch(alph)
    {
        case 'a':
        case 'e':
        case 'i':
        case 'o':
        case 'u':
        case 'A':
        case 'E':
        case 'I':
        case 'O':
        case 'U':
        printf("The character %c is vowel",alph);
        break;
default:
        printf(" The character %c is consonant",alph);
        }
return 0;



}

